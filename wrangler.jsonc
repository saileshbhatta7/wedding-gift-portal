{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "webapp",
  "compatibility_date": "2026-02-07",
  "pages_build_output_dir": "./dist",
  "compatibility_flags": [
    "nodejs_compat"
  ],
  "d1_databases": [
    {
      "binding": "DB",
      "database_name": "wedding-gift-production",
      "database_id": ""
    }
  ],
  "r2_buckets": [
    {
      "binding": "STORAGE",
      "bucket_name": "wedding-gift-uploads"
    }
  ]
}